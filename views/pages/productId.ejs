<% include ../partials/header %>
<div class="container">
    <h1><%= detail.ProductName %></h1>
    <div class="form-group">
        <label for="">ID</label>
        <input type="text" value=<%= detail.ProductID %> readonly class="form-control" name="" id=""
            aria-describedby="helpId" placeholder="">
        <small id="helpId" class="form-text text-muted">Help text</small>
    </div>

    <div class="form-group">
        <label for="">Name</label>
        <input type="text" value=<%= detail.ProductName %> readonly class="form-control" name="" id=""
            aria-describedby="helpId" placeholder="">
        <small id="helpId" class="form-text text-muted">Help text</small>
    </div>

    <div class="form-group">
        <label for="">Quantity Per Unit</label>
        <input type="text" value=<%= detail.QuantityPerUnit %> readonly class="form-control" name="" id=""
            aria-describedby="helpId" placeholder="">
        <small id="helpId" class="form-text text-muted">Help text</small>
    </div>

    <div class="form-group">
            <label for="">Unit Price</label>
            <input type="text" value= <%= detail.UnitPrice %> readonly
              class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help text</small>
          </div>

          <div class="form-group">
                <label for="">Units in Stock</label>
                <input type="text" value= <%= detail.UnitsInStock %> readonly
                  class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
                <small id="helpId" class="form-text text-muted">Help text</small>
              </div>

              <a name="" id="" class="btn btn-primary" href="/products" role="button">Back to Product List</a>
    
</div>

<% include ../partials/footer %>